<?xml version="1.0" encoding="utf-8"?>
<composite name="TestServerComposite" xmlns="http://docs.oasis-open.org/ns/opencsa/sca/200912" targetNamespace="TestServerNamespace">

  <component name="ServerComp1">
    <implementation.cpp
        class="Trentino::Example::DiscoveryTestServerImpl"
		header="DiscoveryTestServerImpl.h"
        scope="composite"
		library="DiscoveryTestServer" />
        
      <service name="DiscoveryTestServer">
      <metadata>
		  <stringAttributes name="color" value="yellow"/>
		</metadata>
      </service>        
     <property name="Color" value="yellow"/>   
